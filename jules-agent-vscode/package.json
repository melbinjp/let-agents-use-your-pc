{
  "name": "jules-agent",
  "displayName": "Jules Endpoint Agent",
  "description": "Easily turn your machine into a secure execution endpoint for the Jules AI agent.",
  "version": "0.1.0",
  "publisher": "jules-ai",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jules.agent.start",
    "onCommand:jules.agent.stop",
    "onCommand:jules.agent.logs"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jules.agent.start",
        "title": "Jules Agent: Start Endpoint"
      },
      {
        "command": "jules.agent.stop",
        "title": "Jules Agent: Stop Endpoint"
      },
      {
        "command": "jules.agent.logs",
        "title": "Jules Agent: View Logs"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
